local Pickups = game:GetService("Workspace").Pickups
_G.Enabled = true
local lplr = game.Players.LocalPlayer
function GetCoin()
local Closest = nil
local ClosestMag = nil
local ClosestPart = nil
for i,v in pairs(Pickups:GetChildren()) do
if v.Name == "Part" and v:FindFirstChild("TouchInterest") then
--print("LOL")
if (v.Position - lplr.Character.HumanoidRootPart.Position).Magnitude < 1000 then
if not Closest then
Closest = v.CFrame
ClosestPart = v
ClosestMag = (v.Position - lplr.Character.HumanoidRootPart.Position).Magnitude
elseif (v.Position - lplr.Character.HumanoidRootPart.Position).Magnitude < ClosestMag then
Closest = v.CFrame
ClosestPart = v
ClosestMag = (v.Position - lplr.Character.HumanoidRootPart.Position).Magnitude
end
end
end
end
game:GetService("TweenService"):Create(lplr.Character.HumanoidRootPart, TweenInfo.new(1, Enum.EasingStyle.Linear), {CFrame = Closest}):Play()
end
while _G.Enabled do
print("ok")
wait(1)
GetCoin()
end
